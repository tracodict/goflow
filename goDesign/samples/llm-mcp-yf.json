{
  "tools": [
    {
      "name": "yahoo_search",
      "description": "Search for symbols, companies, news, and articles",
      "inputSchema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string"
          },
          "options": {
            "type": "object",
            "properties": {
              "lang": {
                "type": "string"
              },
              "region": {
                "type": "string"
              },
              "quotesCount": {
                "type": "number"
              },
              "newsCount": {
                "type": "number"
              },
              "enableFuzzyQuery": {
                "type": "boolean"
              }
            },
            "additionalProperties": false
          }
        },
        "required": [
          "query"
        ],
        "additionalProperties": false,
        "$schema": "http://json-schema.org/draft-07/schema#"
      }
    },
    {
      "name": "yahoo_quote",
      "description": "Get essential symbol information",
      "inputSchema": {
        "type": "object",
        "properties": {
          "symbol": {
            "anyOf": [
              {
                "type": "string"
              },
              {
                "type": "array",
                "items": {
                  "type": "string"
                }
              }
            ]
          },
          "options": {
            "type": "object",
            "properties": {
              "fields": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "return": {
                "type": "string",
                "enum": [
                  "object",
                  "array"
                ]
              }
            },
            "additionalProperties": false
          }
        },
        "required": [
          "symbol"
        ],
        "additionalProperties": false,
        "$schema": "http://json-schema.org/draft-07/schema#"
      }
    },
    {
      "name": "yahoo_historical",
      "description": "Retrieve historical market prices",
      "inputSchema": {
        "type": "object",
        "properties": {
          "symbol": {
            "type": "string"
          },
          "options": {
            "type": "object",
            "properties": {
              "period1": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "number"
                  },
                  {
                    "type": "string",
                    "format": "date-time"
                  }
                ]
              },
              "period2": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "number"
                  },
                  {
                    "type": "string",
                    "format": "date-time"
                  }
                ]
              },
              "interval": {
                "type": "string"
              },
              "includeAdjustedClose": {
                "type": "boolean"
              }
            },
            "required": [
              "period1"
            ],
            "additionalProperties": false
          }
        },
        "required": [
          "symbol"
        ],
        "additionalProperties": false,
        "$schema": "http://json-schema.org/draft-07/schema#"
      }
    },
    {
